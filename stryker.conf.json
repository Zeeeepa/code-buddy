{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker/master/packages/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress", "dashboard"],
  "testRunner": "jest",
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js",
    "enableFindRelatedTests": true
  },
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.d.ts",
    "!src/types/**/*.ts",
    "!src/index.ts"
  ],
  "ignorePatterns": [
    "node_modules",
    "dist",
    "coverage",
    "tests"
  ],
  "timeoutMS": 60000,
  "timeoutFactor": 2,
  "concurrency": 4,
  "logLevel": "info",
  "htmlReporter": {
    "fileName": "reports/mutation/mutation.html"
  },
  "dashboard": {
    "project": "github.com/phuetz/code-buddy",
    "version": "main",
    "module": "code-buddy"
  },
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  },
  "mutator": {
    "excludedMutations": [
      "StringLiteral",
      "ObjectLiteral"
    ]
  },
  "disableTypeChecks": "{src/**/*.ts,src/**/*.tsx}",
  "checkers": ["typescript"],
  "tsconfigFile": "tsconfig.json",
  "incremental": true,
  "incrementalFile": ".stryker-cache/incremental.json",
  "cleanTempDir": true,
  "tempDirName": ".stryker-tmp"
}
